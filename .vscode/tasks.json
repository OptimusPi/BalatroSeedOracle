{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "restore",
			"command": "dotnet",
			"type": "process",
			"args": [
				"restore",
				"${workspaceFolder}/src/BalatroSeedOracle.csproj"
			],
			"group": "build",
			"presentation": { "reveal": "silent" }
		},
		{
			"label": "build (dotnet)",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/src/BalatroSeedOracle.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"group": "build",
			"presentation": { "reveal": "silent" },
			"problemMatcher": "$msCompile"
		}
	]
}