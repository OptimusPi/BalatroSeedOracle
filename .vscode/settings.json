{
    "avalonia.projectPath": "./src/BalatroSeedOracle/BalatroSeedOracle.csproj",
    "avalonia.enableXamlHotReload": true,
    "dotnet.preferCSharpExtension": true,
    "dotnet.unitTests.enabled": true,
    "files.associations": {
        "*.axaml": "AXAML"
    },
    "xml.fileAssociations": [
        {
            "pattern": "**/*.axaml",
            "systemId": "https://github.com/AvaloniaUI/Avalonia/blob/master/src/Markup/Avalonia.Markup.Xaml.Loader/AvaloniaXamlSchema.xsd"
        }
    ],
    "dotnet.server.useOmnisharp": false,
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "terminal.integrated.gpuAcceleration": "on",
    "editor.experimentalGpuAcceleration": "off",
    "terminal.integrated.enableImages": true,
    "claude.exclude": [
        "**/node_modules/**",
        "**/bin/**",
        "**/obj/**",
        "**/.git/**",
        "**/packages/**",
        "**/Releases/**",
        "**/publish/**",
        "**/external/**",
        "**/wwwroot/_framework/**",
        "**/wwwroot/_content/**",
        "**/wwwroot/BSO/_framework/**",
        "**/wwwroot/BSO/wwwroot/**",
        "**/SearchResults/**",
        "**/WordLists/**",
        "**/runtimes/**",
        "**/fr-FR/**",
        "**/ja-JP/**",
        "**/pt-PT/**",
        "**/zh-Hans/**",
        "**/*.dll",
        "**/*.exe",
        "**/*.pdb",
        "**/*.cache",
        "**/*.wasm",
        "**/*.db",
        "**/*.db-wal"
    ],
    "dotnet.defaultSolution": "BalatroSeedOracle.sln",
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "files.watcherExclude": {
        "**/bin/**": true,
        "**/obj/**": true,
        "**/external/**": true,
        "**/node_modules/**": true,
        "**/.git/**": true,
        "**/packages/**": true,
        "**/wwwroot/_framework/**": true,
        "**/wwwroot/_content/**": true,
        "**/wwwroot/BSO/_framework/**": true,
        "**/wwwroot/BSO/wwwroot/**": true,
        "**/*.wasm": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.db": true,
        "**/*.db-wal": true,
        "**/SearchResults/**": true,
        "**/WordLists/**": true,
        "**/runtimes/**": true,
        "**/fr-FR/**": true,
        "**/ja-JP/**": true,
        "**/pt-PT/**": true,
        "**/zh-Hans/**": true
    },
    "files.exclude": {
        "**/bin/**": true,
        "**/obj/**": true,
        "**/external/**": true,
        "**/node_modules/**": true,
        "**/packages/**": true,
        "**/wwwroot/_framework/**": true,
        "**/wwwroot/_content/**": true,
        "**/wwwroot/BSO/_framework/**": true,
        "**/wwwroot/BSO/wwwroot/**": true,
        "**/*.wasm": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.db": true,
        "**/*.db-wal": true,
        "**/SearchResults/**": true,
        "**/WordLists/**": true,
