<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:BalatroSeedOracle.ViewModels"
             xmlns:components="using:BalatroSeedOracle.Components"
             x:Class="BalatroSeedOracle.Views.Modals.FilterSelectionModal"
             x:DataType="vm:FilterSelectionModalViewModel">

    <!-- Full screen modal with Balatro-style overlay -->
    <Grid HorizontalAlignment="Stretch"
          VerticalAlignment="Stretch">
        <!-- Overlay background -->
        <Border x:Name="OverlayBackground"
                Background="{StaticResource SemiTransparentBlack}"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                IsHitTestVisible="False" />

        <!-- Modal positioning grid -->
        <Grid x:Name="ModalContainer">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="45"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="35"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="35"/>
            </Grid.RowDefinitions>

            <!-- Main modal container -->
            <Border x:Name="ModalBorder"
                    Grid.Column="1" Grid.Row="1"
                    Background="{StaticResource ModalGrey}"
                    CornerRadius="20"
                    BorderBrush="{StaticResource ModalBorder}"
                    BorderThickness="4"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Width="1080"
                    Height="604"
                    BoxShadow="{StaticResource StandardModalShadow}"
                    ClipToBounds="False"
                    Padding="4,4,4,0">

                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>    <!-- Content -->
                        <RowDefinition Height="56"/>   <!-- Footer fixed height -->
                    </Grid.RowDefinitions>

                    <!-- Content area with 2-column layout -->
                    <Grid Grid.Row="0" Margin="20">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="250"/>  <!-- Left: Filter List -->
                            <ColumnDefinition Width="20"/>   <!-- Spacer -->
                            <ColumnDefinition Width="*"/>    <!-- Right: Details Panel -->
                        </Grid.ColumnDefinitions>

                        <!-- LEFT SIDE: Filter List (inline implementation) -->
                        <Border Grid.Column="0"
                                BorderThickness="0"
                                CornerRadius="8"
                                Padding="20" ClipToBounds="False">
                            <Grid ClipToBounds="False">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>    <!-- Filter List -->
                                    <RowDefinition Height="Auto"/> <!-- Pagination -->
                                </Grid.RowDefinitions>

                                <!-- Filter List -->
                                <ItemsControl Grid.Row="0" ItemsSource="{Binding FilterList.CurrentPageFilters}" ClipToBounds="False">
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate>
                                            <!-- Wrapper with padding to reserve space for triangle -->
                                            <Grid HorizontalAlignment="Center" Margin="20,0,0,0">
                                                <!-- Button with text (always in same position) -->
                                                <Button Classes="filter-list-item"
                                                        Content="{Binding DisplayText}"
                                                        IsPressed="{Binding IsSelected}"
                                                        Command="{Binding $parent[UserControl].DataContext.FilterList.SelectFilterCommand}"
                                                        CommandParameter="{Binding}"/>

                                                <!-- Triangle indicator (overlaid on left, doesn't push button) -->
                                                <Polygon Points="0,0 15,10 0,20"
                                                         Fill="{StaticResource Red}"
                                                         Classes="balatro-bounce-horizontal"
                                                         HorizontalAlignment="Left"
                                                         VerticalAlignment="Center"
                                                         Margin="-20,0,0,0"
                                                         IsVisible="{Binding IsSelected}"
                                                         IsHitTestVisible="False"/>
                                            </Grid>
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>

                                <!-- Pagination Controls -->
                                <StackPanel Grid.Row="1"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Center"
                                            Spacing="4"
                                            Margin="0,35,0,0">
                                    <Button Content="&lt;"
                                            Classes="btn-red"
                                            Width="44"
                                            Height="36"
                                            FontSize="16"
                                            Padding="0"
                                            CornerRadius="8"
                                            Command="{Binding FilterList.PreviousPageCommand}"/>

                                    <!-- Page indicator -->
                                    <Border Background="{StaticResource Red}"
                                            BorderBrush="Transparent"
                                            BorderThickness="0"
                                            CornerRadius="8"
                                            Padding="16,8"
                                            MinWidth="100"
                                            VerticalAlignment="Center">
                                        <TextBlock Text="{Binding FilterList.PageIndicatorText}"
                                                   FontFamily="{StaticResource BalatroFont}"
                                                   FontSize="14"
                                                   Foreground="White"
                                                   TextAlignment="Center"/>
                                    </Border>

                                    <Button Content="&gt;"
                                            Classes="btn-red"
                                            Width="44"
                                            Height="36"
                                            FontSize="16"
                                            Padding="0"
                                            CornerRadius="8"
                                            Command="{Binding FilterList.NextPageCommand}"/>
                                </StackPanel>
                            </Grid>
                        </Border>

                        <!-- RIGHT SIDE: Details Panel -->
                        <Border Grid.Column="2"
                                Background="{StaticResource DarkBackground}"
                                BorderThickness="0"
                                CornerRadius="8"
                                Padding="20" ClipToBounds="False">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/> <!-- Header: Name/Author/Date -->
                                    <RowDefinition Height="Auto"/> <!-- Tab Buttons with Triangle -->
                                    <RowDefinition Height="*"/>    <!-- Tab Content Area -->
                                    <RowDefinition Height="Auto"/> <!-- Action Buttons -->
                                </Grid.RowDefinitions>

                                <!-- HEADER SECTION (visible when filter selected) -->
                                <StackPanel Grid.Row="0" Spacing="8" Margin="0,0,0,15"
                                            IsVisible="{Binding ShowDetailsPanel}">

                                    <!-- Horizontal Sprite Strips for Must[] Items -->
                                    <!-- JOKERS Strip -->
                                    <Border Background="{StaticResource DarkTealGrey}"
                                            CornerRadius="6"
                                            Padding="12,8"
                                            Margin="0,8,0,0">
                                        <StackPanel Orientation="Horizontal" Spacing="8">
                                            <TextBlock Text="JOKERS"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="12"
                                                       Foreground="{StaticResource Gold}"
                                                       VerticalAlignment="Center"
                                                       FontWeight="Bold"/>
                                            <TextBlock Text="[Joker sprites]"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="11"
                                                       Foreground="{StaticResource White}"
                                                       VerticalAlignment="Center"
                                                       FontStyle="Italic"/>
                                        </StackPanel>
                                    </Border>

                                    <!-- CONSUMABLES Strip -->
                                    <Border Background="{StaticResource DarkTealGrey}"
                                            CornerRadius="6"
                                            Padding="12,8">
                                        <StackPanel Orientation="Horizontal" Spacing="8">
                                            <TextBlock Text="CONSUMABLES"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="12"
                                                       Foreground="{StaticResource Purple}"
                                                       VerticalAlignment="Center"
                                                       FontWeight="Bold"/>
                                            <TextBlock Text="[Consumable sprites]"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="11"
                                                       Foreground="{StaticResource White}"
                                                       VerticalAlignment="Center"
                                                       FontStyle="Italic"/>
                                        </StackPanel>
                                    </Border>

                                    <!-- VOUCHERS Strip -->
                                    <Border Background="{StaticResource DarkTealGrey}"
                                            CornerRadius="6"
                                            Padding="12,8">
                                        <StackPanel Orientation="Horizontal" Spacing="8">
                                            <TextBlock Text="VOUCHERS"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="12"
                                                       Foreground="{StaticResource VoucherOrange}"
                                                       VerticalAlignment="Center"
                                                       FontWeight="Bold"/>
                                            <TextBlock Text="[Voucher sprites]"
                                                       FontFamily="{StaticResource BalatroFont}"
                                                       FontSize="11"
                                                       Foreground="{StaticResource White}"
                                                       VerticalAlignment="Center"
                                                       FontStyle="Italic"/>
                                        </StackPanel>
                                    </Border>
                                </StackPanel>

                                <!-- TAB NAVIGATION (visible when filter selected) -->
                                <components:BalatroTabControl Grid.Row="1"
                                                              DataContext="{Binding TabControl}"
                                                              IsVisible="{Binding $parent[Grid].DataContext.ShowDetailsPanel}"
                                                              Margin="0,0,0,10"/>

                                <!-- TAB CONTENT AREA -->
                                <Grid Grid.Row="2">
                                    <!-- FILTER TAB CONTENT (two-column layout) -->
                                    <ScrollViewer VerticalScrollBarVisibility="Auto"
                                                  IsVisible="{Binding ShowFilterTab}">
                                        <Grid ColumnDefinitions="*,15,*">
                                            <!-- Left Column: Filter Description -->
                                            <Border Grid.Column="0"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="Filter Description"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="10">
                                                        <!-- Filter Name (title, big font) -->                                                        <TextBlock Text="{Binding FilterName}"                                                                   FontFamily="{StaticResource BalatroFont}"                                                                   FontSize="18"                                                                   FontWeight="Bold"                                                                   Foreground="Black"                                                                   TextWrapping="Wrap"/>
                                                        <!-- Author (at top, bold, FontSize 16) -->
                                                        <TextBlock FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="16"
                                                                   FontWeight="Bold"
                                                                   Foreground="Black">
                                                            <Run Text="by "/>
                                                            <Run Text="{Binding FilterAuthor}"/>
                                                        </TextBlock>

                                                        <!-- Created Date (below author, lighter, FontSize 12) -->
                                                        <TextBlock FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="12"
                                                                   Foreground="#666666">
                                                            <Run Text="Created: "/>
                                                            <Run Text="{Binding CreatedDate}"/>
                                                        </TextBlock>

                                                        <!-- Description Text -->
                                                        <TextBlock Text="{Binding FilterDescription}"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="13"
                                                                   Foreground="Black"
                                                                   TextWrapping="Wrap"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>

                                            <!-- Right Column: Filter Criteria (Must[]) -->
                                            <Border Grid.Column="2"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="Filter Criteria"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="8">
                                                        <TextBlock FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="13"
                                                                   Foreground="Black">
                                                            <Run Text="Must Have: "/>
                                                            <Run Text="{Binding MustHaveCount}"/>
                                                            <Run Text=" items"/>
                                                        </TextBlock>
                                                        <TextBlock Text="[Item sprites will display here]"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="11"
                                                                   Foreground="#666666"
                                                                   FontStyle="Italic"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                    </ScrollViewer>

                                    <!-- SCORE TAB CONTENT (two-column layout) -->
                                    <ScrollViewer VerticalScrollBarVisibility="Auto"
                                                  IsVisible="{Binding ShowScoreTab}">
                                        <Grid ColumnDefinitions="*,15,*">
                                            <!-- Left Column: Scoring Description -->
                                            <Border Grid.Column="0"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="Scoring Description"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="10">
                                                        <TextBlock Text="Items in the Should[] clause contribute to the seed's score. Higher scores indicate seeds that better match your preferences."
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="13"
                                                                   Foreground="Black"
                                                                   TextWrapping="Wrap"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>

                                            <!-- Right Column: Scoring Criteria (Should[]) -->
                                            <Border Grid.Column="2"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="Scoring Criteria"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="8">
                                                        <TextBlock FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="13"
                                                                   Foreground="Black">
                                                            <Run Text="Should Have: "/>
                                                            <Run Text="{Binding ShouldHaveCount}"/>
                                                            <Run Text=" items"/>
                                                        </TextBlock>
                                                        <TextBlock Text="[Item sprites will display here]"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="11"
                                                                   Foreground="#666666"
                                                                   FontStyle="Italic"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                    </ScrollViewer>

                                    <!-- DECK TAB CONTENT (three-column layout) -->
                                    <ScrollViewer VerticalScrollBarVisibility="Auto"
                                                  IsVisible="{Binding ShowDeckTab}">
                                        <Grid ColumnDefinitions="*,10,*,10,*">
                                            <!-- Column 1: Jokers -->
                                            <Border Grid.Column="0"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="JOKERS"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="8">
                                                        <TextBlock Text="[Joker sprites will display here]"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="11"
                                                                   Foreground="#666666"
                                                                   FontStyle="Italic"
                                                                   TextAlignment="Center"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>

                                            <!-- Column 2: Consumables -->
                                            <Border Grid.Column="2"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="CONSUMABLES"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="8">
                                                        <TextBlock Text="[Consumable sprites will display here]"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="11"
                                                                   Foreground="#666666"
                                                                   FontStyle="Italic"
                                                                   TextAlignment="Center"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>

                                            <!-- Column 3: Vouchers -->
                                            <Border Grid.Column="4"
                                                    Background="#FFFFFF"
                                                    CornerRadius="8">
                                                <Grid RowDefinitions="Auto,*">
                                                    <!-- Header -->
                                                    <Border Grid.Row="0"
                                                            Background="{StaticResource Blue}"
                                                            CornerRadius="8,8,0,0"
                                                            Padding="12,8">
                                                        <TextBlock Text="VOUCHERS"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="14"
                                                                   Foreground="White"
                                                                   HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <!-- Content -->
                                                    <StackPanel Grid.Row="1" Margin="15" Spacing="8">
                                                        <TextBlock Text="[Voucher sprites will display here]"
                                                                   FontFamily="{StaticResource BalatroFont}"
                                                                   FontSize="11"
                                                                   Foreground="#666666"
                                                                   FontStyle="Italic"
                                                                   TextAlignment="Center"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                    </ScrollViewer>

                                    <!-- Placeholder when no filter selected (Balatro Challenges style) -->
                                    <StackPanel HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Spacing="15"
                                                IsVisible="{Binding ShowPlaceholder}">
                                        <TextBlock Text="Select a Filter"
                                                   FontFamily="{StaticResource BalatroFont}"
                                                   FontSize="20"
                                                   Foreground="{StaticResource White}"
                                                   HorizontalAlignment="Center"
                                                   TextAlignment="Center"/>

                                        <TextBlock Text="— or —"
                                                   FontFamily="{StaticResource BalatroFont}"
                                                   FontSize="16"
                                                   Foreground="{StaticResource TextInactive}"
                                                   HorizontalAlignment="Center"
                                                   TextAlignment="Center"/>

                                        <Button Content="+ CREATE NEW FILTER"
                                                Classes="btn-blue"
                                                FontSize="16"
                                                Height="50"
                                                MinWidth="280"
                                                Command="{Binding CreateNewCommand}"/>
                                    </StackPanel>
                                </Grid>

                                <!-- ACTION BUTTONS (bottom right, BELOW tabs) -->
                                <StackPanel Grid.Row="3"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Right"
                                            Spacing="8"
                                            Margin="0,20,0,0"
                                            IsVisible="{Binding ShowDetailsPanel}">

                                    <!-- PRIMARY ACTION BUTTON (context-specific) -->
                                    <!-- For DESIGNER: "Make a Copy of this Filter" -->
                                    <Button Content="Make a Copy of this Filter"
                                            Classes="btn-blue"
                                            Height="45"
                                            MinWidth="280"
                                            FontSize="15"
                                            Command="{Binding CopyCommand}"
                                            IsVisible="{Binding EnableCopy}"/>

                                    <!-- For SEARCH: "Search Seeds using this Filter" -->
                                    <Button Content="Search Seeds using this Filter"
                                            Classes="btn-blue"
                                            Height="45"
                                            MinWidth="280"
                                            FontSize="15"
                                            Command="{Binding SearchCommand}"
                                            IsVisible="{Binding EnableSearch}"/>

                                    <!-- SECONDARY BUTTONS -->
                                    <!-- Edit Button (yellow-orange) -->
                                    <Button Content="Edit this Filter"
                                            Classes="btn-orange"
                                            Height="45"
                                            MinWidth="160"
                                            FontSize="14"
                                            Command="{Binding EditCommand}"
                                            IsVisible="{Binding EnableEdit}"/>

                                    <!-- Delete Button (red) -->
                                    <Button Content="Delete this Filter"
                                            Classes="btn-red"
                                            Height="45"
                                            MinWidth="160"
                                            FontSize="14"
                                            Command="{Binding DeleteCommand}"
                                            IsVisible="{Binding EnableDelete}"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>

                    <!-- Footer with BACK button -->
                    <Button Grid.Row="1"
                            Content="BACK"
                            Classes="back-button"
                            HorizontalAlignment="Stretch"
                            Command="{Binding BackCommand}"
                            Margin="0"/>
                </Grid>
            </Border>
        </Grid>
    </Grid>

    <UserControl.Styles>
        <!-- Filter list item style (Balatro Challenges row spec) -->
        <Style Selector="Button.filter-list-item">
            <Setter Property="Background" Value="{StaticResource Red}"/>
            <Setter Property="Foreground" Value="{StaticResource White}"/>
            <Setter Property="FontFamily" Value="{StaticResource BalatroFont}"/>
            <Setter Property="FontSize" Value="13"/>
            <Setter Property="Height" Value="26"/>
            <Setter Property="Margin" Value="0,2"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="MinWidth" Value="210"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="12,2"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="BorderThickness" Value="0"/>
        </Style>

        <Style Selector="Button.filter-list-item:pointerover">
            <Setter Property="Background" Value="#8b0000"/>
        </Style>

        <Style Selector="Button.filter-list-item:pressed">
            <Setter Property="Background" Value="#660000"/>
        </Style>

        <!-- Special blue styling for CREATE NEW FILTER -->
        <Style Selector="Button.create-new-item">
            <Setter Property="Background" Value="{StaticResource Blue}"/>
        </Style>

        <Style Selector="Button.create-new-item:pointerover">
            <Setter Property="Background" Value="{StaticResource Blue}"/>
        </Style>

        <Style Selector="Button.create-new-item:pressed">
            <Setter Property="Background" Value="{StaticResource Blue}"/>
        </Style>
    </UserControl.Styles>

</UserControl>
